def nums_generator(n):
    for num in range(1, n + 1, 2):
        yield num


odd_to_15 = nums_generator(15)

print(type(odd_to_15))

print(next(odd_to_15))
print(next(odd_to_15))
print(next(odd_to_15))
print(next(odd_to_15))
print(next(odd_to_15))
print(next(odd_to_15))
print(next(odd_to_15))
print(next(odd_to_15))
print(next(odd_to_15))
